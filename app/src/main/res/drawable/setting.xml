<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"


    >

    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/透明" />
            <size
                android:width="40dp"
                android:height="40dp" />
        </shape>
    </item>

    <item
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp">
        <shape
            android:innerRadius="10dp"
            android:shape="ring"
            android:thickness="5dp"
            android:useLevel="false">
            <solid android:color="@color/plus_sign_color" />
        </shape>
    </item>

    <item
        android:bottom="30dp"
        android:left="17dp"
        android:right="17dp"
        android:top="0dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/plus_sign_color" />
        </shape>
    </item>

    <item
        android:bottom="0dp"
        android:left="17dp"
        android:right="17dp"
        android:top="30dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/plus_sign_color" />
        </shape>
    </item>

    <item
        android:bottom="17dp"
        android:left="0dp"
        android:right="30dp"
        android:top="17dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/plus_sign_color" />
        </shape>
    </item>

    <item
        android:bottom="17dp"
        android:left="30dp"
        android:right="0dp"
        android:top="17dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/plus_sign_color" />
        </shape>
    </item>
    <item
        android:bottom="26dp"
        android:left="28dp"
        android:right="7dp"
        android:top="4dp">
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="@color/plus_sign_color" />
            </shape>
        </rotate>
    </item>

    <item
        android:bottom="4dp"
        android:left="28dp"
        android:right="7dp"
        android:top="26dp">
        <rotate android:fromDegrees="135">
            <shape android:shape="rectangle">
                <solid android:color="@color/plus_sign_color" />
            </shape>
        </rotate>
    </item>

    <item
        android:bottom="4dp"
        android:left="7dp"
        android:right="28dp"
        android:top="26dp">
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="@color/plus_sign_color" />
            </shape>
        </rotate>
    </item>


    <item
        android:bottom="26dp"
        android:left="7dp"
        android:right="28dp"
        android:top="4dp">
        <rotate android:fromDegrees="315">
            <shape android:shape="rectangle">
                <solid android:color="@color/plus_sign_color" />
            </shape>
        </rotate>
    </item>

</layer-list>